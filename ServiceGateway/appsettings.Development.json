{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "Spring": {
    "Application": {
      "Name": "SERVICEGATEWAY"
    }
  },
  "Eureka": {
    "Client": {
      "ServiceUrl": {
        "DefaultZone": "http://localhost:8761/eureka/"
      },
      "ValidateCertificates": false,
      "ShouldRegisterWithEureka": true
    },
    "Instance": {
      "NonSecurePort": 5015,
      "HostName": "localhost",
      "InstanceId": "ServiceGateway,Port:5015"
    }
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200"
  }
}
